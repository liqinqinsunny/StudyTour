#import "WJPHomePageSectionHeaderView.h"

@interface WJPHomePageSectionHeaderView ()
@property (weak, nonatomic) IBOutlet UILabel *headerTitleLabel;

@end

@implementation WJPHomePageSectionHeaderView

- (void)setSectionHeaderTitle:(NSString *)sectionHeaderTitle
{
    if ([_sectionHeaderTitle isEqualToString:sectionHeaderTitle]) {
        return;
    }
    _sectionHeaderTitle = sectionHeaderTitle;
    _headerTitleLabel.text = _sectionHeaderTitle;
}

@end
